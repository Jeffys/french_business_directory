<odoo>
  <data>
    <record id="view_email_server_form_inherit" model="ir.ui.view">
        <field name="name">fetchmail.server.form.inherit</field>
        <field name="model">fetchmail.server</field>
        <field name="inherit_id" ref="mail.view_email_server_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='attach']" position="after">
                <field name="mark_read" groups="base.group_no_one"/>
            </xpath>
        </field>
    </record>
  </data>
</odoo>
